{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "enabledManagers": [
        "cargo"
    ],
	"bumpVersion": "patch",
    "forkProcessing": "enabled",
    "extends": [
        "config:recommended"
    ],
    "updatePinnedDependencies": false,
    "packageRules": [
        {
            "automerge": true,
            "matchUpdateTypes": [
                "minor",
                "patch"
            ]
        }
    ]
}
